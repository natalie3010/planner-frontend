{
  "supplyForm": {
    "inputs": {
      "supplyFName": {
        "validators": [
          {
            "errorDisplayed": "Please put a first name",
            "required": true
          }
        ]
      },
      "supplyLName": {
        "validators": [
          {
            "errorDisplayed": "Please put a last name",
            "required": true
          }
        ]
      },
      "supplyStatus": {
        "validators": [
          {
            "errorDisplayed": "Please select a status",
            "required": true
          }
        ]
      },
      "supplySkillId": {
        "validators": [
          {
            "errorDisplayed": "Please select a skill",
            "required": true
          }
        ]
      },
      "supplyNotes": {
        "validators": [
          {
            "required": false
          }
        ]
      },
      "supplyType": {
        "validators": [
          {
            "errorDisplayed": "Please select a type",
            "required": true
          }
        ]
      },
      "supplyLocation": {
        "validators": [
          {
            "required": false
          }
        ]
      }
    }
  },

  "demandForm": {
    "inputs": {
      "demandCodeRequisition": {
        "validators": [{ "required": false }]
      },
      "demandStartDate": {
        "validators": [
          {
            "required": true,
            "errorDisplayed": "Please put a valid date following the pattern dd/MM/yyyy",
            "pattern": "(([12][0-9]{1})|(0[1-9]{1})|(3[01]{1}))\\/((0[1-9])|(1[012]))\\/(2([0-9]{3}))"
          }
        ]
      },
      "demandOriginator": {
        "validators": [{ "required": false }]
      },
      "demandProbability": {
        "validators": [{ "required": false }]
      },
      "demandSelectedApplicant": {
        "validators": [{ "required": false }]
      },
      "demandNotes": {
        "validators": [{ "required": false }]
      },
      "demandProposedApplicant": {
        "validators": [{ "required": false }]
      },
      "demandCreationDate": {
        "validators": [
          {
            "errorDisplayed": "Please put a valid date following the pattern dd/MM/yyyy",
            "required": true,
            "pattern": "(([12][0-9]{1})|(0[1-9]{1})|(3[01]{1}))\\/((0[1-9])|(1[012]))\\/(2([0-9]{3}))"
          }
        ]
      },
      "demandLocation": {
        "validators": [{ "required": false }]
      },
      "demandSkill": {
        "validators": [{ "required": false }]
      },
      "demandGrade": {
        "validators": [{ "required": false }]
      },
      "demandStatus": {
        "validators": [{ "required": false }]
      }
    }
  },
  "loginForm": {
    "inputs": {
      "usernameLogin": {
        "validators": [
          {
            "required": true
          }
        ]
      },
      "passwordLogin": {
        "validators": [
          {
            "required": true
          }
        ]
      }
    }
  }
}
